{
  "lerna": "2.10.1",
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "changelog": {
    "repo": "jleveugle/manager-test",
    "labels": {
      "feat": "New Feature",
      "fix": "Bug Fix"
    }
  },
  "command": {
    "bootstrap": {
      "npmClientArgs": ["--access public"]
    },
    "publish": {
      "conventionalCommits": true
    }
  }
}
